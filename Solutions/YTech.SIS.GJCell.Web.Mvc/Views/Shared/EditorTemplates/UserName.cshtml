@model string
           
@(
 Html.Kendo().DropDownListFor(m => m)  
        .OptionLabel("-Pilih User-")
        .DataValueField("UserName")
        .DataTextField("UserName")     
        .HtmlAttributes(new { @style = "width:200px;" })
         .AutoBind(false)
         .DataSource(source =>
         {
             source.Read(read =>
             {
                 read.Action("PopulateUsers", "Account");
             });
         })
)
